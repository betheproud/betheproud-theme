  <extensions defaultExtensionNs="com.intellij">
    <themeProvider id="e9501a23-fcca-4b66-aaeb-8e7b44a92a10" path="/theme/superdesire.theme.json"/>
  </extensions>

  <actions>

  </actions>

</idea-plugin>
